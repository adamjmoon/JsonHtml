{
    "elements" :
        [
            {
                 "type" : "span",
                 "html" : " Server: "
            },
            {
                 "type" : "select",
                 "id" : "serverList",
                 "style" : "width: 200px; margin:5px;"
            },
            {
                 "type" : "span",
                 "html" : " Database: "
            },
            {
                 "type" : "select",
                 "id" : "dbList",
                 "style" : "width: 200px; margin:5px;"
            },
            {
                 "type" : "span",
                 "html" : " Report: "
            },
            {
