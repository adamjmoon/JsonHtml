                             {
                                 "id" : "sqlTextArea",
                                 "type" : "textarea",
                                 "class": "grid_7",
                                 "style" : "margin: 5px;"

                             },
                              {
                                     "type" : "text",
                                     "id" : "reportName",
                                     "placeholder" : "Report Name",
                                     "html" : "",
                                     "class" : "ui-corner-all ui-widget pad",
                                     "style" : "width: 300px; margin:5px;"
                                },
                              {
                                     "type" : "label",
                                     "id" : "saveReport",
                                     "html" : "Save Report",
                                     "class" : "ui-corner-all ui-widget pad ui-state-focus",
                                     "style" : "width: 100px; margin:5px;"
                                }
