                 "style" : "width: 100px; margin:5px;"
            },
            {
                 "type" : "container",
                 "id" : "fileupload",
                 "html" : "",
                "class" : "ui-corner-all ui-widget pad ui-state-default",
                "style" :  "float: right; margin:5px;"

            },
            {
                 "type" : "tabs",
                 "id" : "tabs",
                 "entries" :
                 {
                     "reportTab":
                     {
                         "caption" : "Report",
                         "elements" :
                         [
                             {
                                 "type" : "container",
                                 "html" : "<div id='datatable-wrapper'></div>"
                             }

                         ]
                     },
                     "sqlTab" :
                     {
                         "id" : "sqlTab",
                         "caption" : "Sql",
                         "elements" :
